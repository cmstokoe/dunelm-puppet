---
accounts:user_defaults:
    purge_ssh_keys: true
    
accounts::users:
  root:
    uid: 0
    pwhash: ENC[PKCS7,] 
    home: '/root'
  dunelm:
    ensure: present
    uid: 999
    groups: ['users']
    pwhash: ENC[PKCS7,]
  ubuntu:
    ensure: absent
  chris:
    uid: 5000
    comment: ENC[PKCS7,]
    groups: ["sudo", "users"]
    pwhash: ENC[PKCS7,] 
    ssh_keys:
      'chris-mb': # an unique indentifier of a key
         type: "ssh-rsa"
         key: ENC[PKCS7,]
  sarah:
    uid: 5001
    comment: ENC[PKCS7,]
    groups: ["sudo", "users"]
    pwhash: ENC[PKCS7,]
    ssh_keys:
      'laaa': # an unique indentifier of a key
         type: "ssh-rsa"
         key: ENC[PKCS7,]
